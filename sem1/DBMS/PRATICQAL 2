create table TBL_STUDENT 
(
STID bigint NOT NULL, 
DEPTID bigint REFERENCES TBL_DEPARTMENT(DEPTID),
STNAME char(50)  NOT NULL, 
DATE_OF_BIRTH DATE  NOT NULL, 
CITY CHAR(50)  NOT NULL, 
EMAIL_ID CHAR(50)  NOT NULL,
CONTACT_NUMBER BIGINT  NOT NULL,
GENDER CHAR(10) NOT NULL
);

CREATE TABLE TBL_DEPARTMENT
(
DEPTID BIGINT NOT NULL, 
DEPT_NAME CHAR(50) NOT NULL,
PRIMARY KEY (DEPTID)
);

CREATE TABLE TBL_EMP
(
EMPID BIGINT,
EMP_NAME CHAR(50),
EMP_DESIGNATION CHAR(50),
DEPTID BIGINT REFERENCES TBL_DEPARTMENT(DEPID),
EMP_SALARY DECIMAL(11,2),
EMP_JD DATE,
EMP_GENDER CHAR(50),
EMP_EXP CHAR(200),
PRIMARY KEY(EMPID)
);

INSERT TBL_DEPARTMENT VALUE
(
3434353536,
'MCA'
);


