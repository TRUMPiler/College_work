CREATE TABLE Tbl_compram
(
Ram_name char(50) NOT NULL,
Ram_speed char(10) NOT NULL,
Ram_price int NOT NULL,
Ram_stock smallint NOT NULL,
Ram_sold smallint NOT NULL
);

CREATE TABLE Tbl_ssd
(
SSD_NAME char(50) NOT NULL,
SSD_space char(10) NOT NULL,
SSD_price int NOT NULL,
SSD_stock smallint NOT NULL,
SSD_sold smallint NOT NULL
);

CREATE TABLE Tbl_graphicard
(
graphicard_name char(50) NOT NULL,
graphicard_speed char(10) NOT NULL,
graphicard_price bigint NOT NULL,
graphicard_stock smallint NOT NULL,
graphicard_sold smallint NOT NULL
);

CREATE TABLE Tbl_cabinet
(
cabinet_name char(50) NOT NULL,
cabinet_type char(10) NOT NULL,
cabinet_price bigint NOT NULL,
cabinet_stock smallint NOT NULL,
cabinet_sold smallint NOT NULL
);

CREATE TABLE Tbl_monitor
(
monitor_name char(50) NOT NULL,
monitor_refreshrate char(10) NOT NULL,
monitor_price bigint NOT NULL,
monitor_stock smallint NOT NULL,
monitor_sold smallint NOT NULL
);

CREATE TABLE Tbl_motherboard
(
motherboard_name char(50) NOT NULL,
motherboard_price bigint NOT NULL,
motherboard_stock smallint NOT NULL,
motherboard_sold smallint NOT NULL
);

CREATE TABLE Tbl_processor
(
processor_name char(50) NOT NULL,
processor_cores smallint NOT NULL,
processor_price int NOT NULL,
processor_stock smallint NOT NULL,
processor_sold smallint NOT NULL
);


