CREATE TABLE Tbl_swiggy
(
customer_ID bigint,
customer_name char(50) NOT NULL,
customer_phoneno bigint NOT NULL,
customer_adress char(250) NOT NULL,
city char(20) NOT NULL,
state char(25) NOT NULL,
country char(50) NOT NULL,
customer_REVIEW char(78) NOT NULL,
customer_prefence char(250) NOT NULL,
customer_allergy char(200) NOT NULL
);

